# SplitFree Environment Variables
# Copy this file to .env and fill in your values

# Supabase Configuration (Required)
EXPO_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
EXPO_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here

# Clerk Authentication (Required)
EXPO_PUBLIC_CLERK_PUBLISHABLE_KEY=pk_test_your-key-here

# OAuth Configuration
EXPO_PUBLIC_OAUTH_REDIRECT_URL=splitfree://oauth-callback

# Gemini API for Receipt OCR (Optional)
EXPO_PUBLIC_GEMINI_API_KEY=your-gemini-key-here
